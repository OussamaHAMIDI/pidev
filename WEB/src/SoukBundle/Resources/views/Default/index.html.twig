{% extends '@Souk/layout.html.twig' %}
{% block body %}

    <!-- /section-title -->

    {% block topTenBoutiques %}
        {{ render(controller(
            'EvaluationBundle\\Controller\\EvaluationController::afficherTopTenBoutiquesAction'
        )) }}
    {% endblock topTenBoutiques %}

    {% block topTenProduits %}
        {{ render(controller(
            'EvaluationBundle\\Controller\\EvaluationController::afficherTopTenProduitsAction'
        )) }}
    {% endblock topTenProduits %}


    <!-- section -->
    <div class="section section-grey">
        <!-- container -->
        <div class="container">
            <!-- row -->
            <div class="row col-md-12 col-md-12">
                <!-- banner -->
                <div class="col-md-8">
                    <div class="banner banner-1">
                        <img src="{{ asset('uploads/images/couffin.jpg') }}"
                             alt="">
                        <div class="banner-caption text-center">
                            <h1 class="primary-color">Souk Lemdina<br><span
                                        class="white-color font-weak">Créé par Hints</span></h1>
                        </div>
                    </div>
                </div>
                <!-- /banner -->

                <!-- banner -->
                <div class="col-md-4 col-sm-6">
                    <a class="banner banner-1" href="#">
                        <img src="{{ asset('uploads/images/b37b8c7e8db00f800e46d3b9b2b08e51.jpeg') }}"
                             alt="">
                        <div class="banner-caption text-center">
                            <h2 class="white-color">Boutiques Souk Lemdina</h2>
                        </div>
                    </a>
                </div>
                <!-- /banner -->

                <!-- banner -->
                <div class="col-md-4 col-sm-6">
                    <a class="banner banner-1" href="#">
                        <img src="{{ asset('uploads/images/chacheya.jpg') }}"
                             alt="">
                        <div class="banner-caption text-center">
                            <h2 class="white-color">Chacheya</h2>
                        </div>
                    </a>
                </div>
                <!-- /banner -->
            </div>
            <!-- /row -->
        </div>
        <!-- /container -->
    </div>
    <!-- /section -->

{% endblock %}
